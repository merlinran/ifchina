.container
  .row
    .col-md-8
      {% paginate entries by 3 %}
      {% for entry in paginate.collection %}
      .panel
        .entry
          %h1 {{ entry.title }}
          {{ entry.photo.url | image_tag }}
          %p {{ entry.text }}
      {% endfor %}
      {{ paginate | default_pagination }}
      {% endpaginate %}
